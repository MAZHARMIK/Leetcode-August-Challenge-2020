class Solution {
public:
    vector<int> getRow(int rowIndex) {
        vector<vector<int>> vec(rowIndex+1, vector<int>(rowIndex+1, 0));
        
        for(int i = 0; i<rowIndex+1; i++){
            vec[i][0] = 1;
        }
        
        for(int i = 0; i<rowIndex; i++) {
            for(int j = 1; j<=rowIndex; j++) {
                vec[i+1][j] = vec[i][j-1] + vec[i][j];
            }
        }
        
        return vec[rowIndex];
    }
};
